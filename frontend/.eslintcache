styles'\nimport HeartImg from '../../assets/Heart.png'\nimport AuthContext from '../../context/AuthContext'\nimport api from '../../services/api'\n\ninterface Props {\n    id: string\n    podcastId: string\n    image: string\n    name: string\n    audioUrl: string\n}\n\nconst useAudio = (url: string) => {\n    const [audio] = useState(new Audio(url))\n    const [playing, setPlaying] = useState<boolean>(false)\n\n    const toggle = () => {\n        setPlaying(!playing)\n\n    }\n\n    useEffect(() => {\n        playing ? audio.play() : audio.pause()\n    },\n    [playing]\n    )\n\n    useEffect(() => {\n        audio.addEventListener('ended', () => setPlaying(false))\n        return () => {\n            audio.removeEventListener('ended', () => setPlaying(false))\n        }\n    }, [])\n\n    return [playing, toggle]\n}\n\nconst Episode: React.FC<Props> = ({id, podcastId, image, name, audioUrl}: Props) => {\n\n    const [playing, toggle] = useAudio(audioUrl)\n    const { logged, id:userId } = useContext(AuthContext)\n\n    const [liked, setLiked] = useState(false)\n    const [likes, setLikes] = useState(0)\n\n    const handleLike = async () => {\n\n        setLiked(!liked)\n\n        liked ? setLikes(likes - 1) : setLikes(likes + 1)\n    }\n\n    return (\n        <EpisodeCard key={id}>\n            <div style={{display: 'flex'}}>\n                <EpisodeImageCard about={ image } />\n                <EpisodeInfo>\n                    <h1>{ name }</h1>\n                    {\n                        logged ? (\n                            <EpisodeStats>\n                                <img src={HeartImg} />\n                                <h1>{ likes }</h1>\n                            </EpisodeStats>\n                        ) : (\n                            <div></div>\n                        )\n                    }\n                </EpisodeInfo>\n            </div>\n            <Buttons>\n                <button onClick={toggle as ((event: React.MouseEvent<HTMLButtonElement, MouseEvent>) => void) | undefined}>\n                    {\n                        playing ? (\n                            <FiPauseCircle size='1.9vw' color='#fff' ></FiPauseCircle>\n                        ) : (\n                            <FiPlayCircle size='1.9vw' color='#fff' ></FiPlayCircle>\n                        )\n                    }\n                </button>\n                {\n                    logged && (\n                        liked ? (\n                            <FiHeart onClick={() => handleLike()} size='1.9vw' color='#fff' fill='#fff' ></FiHeart>\n                        ) : (\n                            <FiHeart onClick={() => handleLike()} size='1.9vw' color='#fff' ></FiHeart>\n                        )\n                    )\n                }\n            </Buttons>\n        </EpisodeCard>\n    )\n}\n\nexport default Episode\n","/home/renan/college/trabalho_bancos_de_dados/frontend/src/pages/Episode/styles.ts",[],"/home/renan/college/trabalho_bancos_de_dados/frontend/src/reportWebVitals.ts",["106"],"import { ReportHandler } from 'web-vitals'\n\nconst reportWebVitals = (onPerfEntry?: ReportHandler) => {\n    if (onPerfEntry && onPerfEntry instanceof Function) {\n        import('web-vitals').then(({ getCLS, getFID, getFCP, getLCP, getTTFB }) => {\n            getCLS(onPerfEntry)\n            getFID(onPerfEntry)\n            getFCP(onPerfEntry)\n            getLCP(onPerfEntry)\n            getTTFB(onPerfEntry)\n        })\n    }\n}\n\nexport default reportWebVitals\n","/home/renan/college/trabalho_bancos_de_dados/frontend/src/services/api.ts",[],[],"/home/renan/college/trabalho_bancos_de_dados/frontend/src/pages/UserLogin/index.tsx",[],"/home/renan/college/trabalho_bancos_de_dados/frontend/src/pages/UserLogin/styles.ts",[],{"ruleId":"107","severity":1,"message":"108","line":73,"column":68,"nodeType":"109","messageId":"110","endLine":73,"endColumn":71,"suggestions":"111"},{"ruleId":"107","severity":1,"message":"108","l